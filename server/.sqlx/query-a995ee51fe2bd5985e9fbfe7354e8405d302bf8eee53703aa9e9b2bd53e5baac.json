  "query": "\n            SELECT\n                a.id,\n                a.article_id,\n                feed_id,\n                title,\n                published AS \"published: OffsetDateTime\",\n                link,\n                read AS \"read!: bool\",\n                saved AS \"saved!: bool\"\n            FROM articles AS a\n            LEFT OUTER JOIN user_articles\n                ON user_articles.article_id = a.id\n            WHERE feed_id = $1\n            ORDER BY published ASC\n            ",
