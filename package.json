{
  "name": "simple-news",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:downloader": "tsc -p scripts/tsconfig.json",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "husky install",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "db": "ts-node ./scripts/db.ts",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "download-feeds": "ts-node ./scripts/download-feeds.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.1.4",
    "@sveltejs/kit": "^1.2.2",
    "@swc/core": "^1.3.27",
    "@types/bcryptjs": "^2.4.2",
    "@types/better-sqlite3": "^7.6.3",
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.11.18",
    "@types/yargs": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "bcryptjs": "^2.4.3",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.2",
    "svelte-preprocess": "^5.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.27.3",
    "yargs": "^17.6.2",
    "zx": "^7.1.1"
  },
  "dependencies": {
    "better-sqlite3": "^8.0.1",
    "cheerio": "1.0.0-rc.12",
    "cuid": "^2.1.8",
    "rss-parser": "^3.12.0",
    "undici": "^5.15.2"
  },
  "type": "module"
}
