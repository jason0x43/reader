{
  "name": "simple-news",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "husky install && prisma generate",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "db": "ts-node ./scripts/db.ts",
    "migrate": "prisma migrate deploy",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "push": "ts-node ./scripts/deploy.ts",
    "download-feeds": "ts-node ./scripts/download-feeds.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.85",
    "@sveltejs/kit": "next",
    "@swc/core": "^1.2.223",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.5.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.6.4",
    "@types/yargs": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "bcryptjs": "^2.4.3",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "prisma": "^4.1.1",
    "rss-parser": "^3.12.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.21.0",
    "yargs": "^17.5.1"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^4.1.1"
  }
}
