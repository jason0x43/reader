{
  "name": "simple-news",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "husky install",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "db": "ts-node ./scripts/db.ts",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "download-feeds": "ts-node ./scripts/download-feeds.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.92",
    "@sveltejs/kit": "next",
    "@swc/core": "^1.3.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/better-sqlite3": "^7.6.0",
    "@types/cookie": "^0.5.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.18",
    "@types/yargs": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "bcryptjs": "^2.4.3",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "rss-parser": "^3.12.0",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.0",
    "svelte-preprocess": "^4.10.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.23.4",
    "yargs": "^17.5.1",
    "zx": "^7.0.8"
  },
  "dependencies": {
    "better-sqlite3": "^7.6.2",
    "cuid": "^2.1.8"
  },
  "type": "module"
}
