{
	"compilerOptions": {
		"target": "es2022",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"declaration": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "build",
		"sourceMap": true,
		"strict": true,
		"allowJs": true,
		"checkJs": true,
		"skipLibCheck": true
	}
}
