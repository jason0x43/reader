{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"plugins": [{ "name": "typescript-svelte-plugin" }]
	},
	"ts-node": {
		"esm": true,
		"swc": true,
		"transpileOnly": true
	}
}
